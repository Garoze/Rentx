{
  "name": "Rentx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/shared/infra/http/server.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/shared/infra/typeorm/index.ts migration:generate ./src/database/migrations/default",
    "migration:run": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -d ./src/shared/infra/typeorm/index.ts",
    "build": "rm -rf ./dist && tsc",
    "start": "node ./dist/index.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed:admin": "ts-node -r tsconfig-paths/register ./src/shared/infra/typeorm/seed/admin.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.3",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.3.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "csv-parse": "^5.3.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.7"
  }
}
